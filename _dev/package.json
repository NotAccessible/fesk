{
  "name": "FESK",
  "version": "1.0.0",
  "description": "Front End Starter Kit",
  "author": "Dragan Eror",
  "license": "MIT",
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-gzip": "^1.4.0",
    "gulp-imagemin": "^3.3.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sass-glob": "^1.0.8",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-svg-sprite": "^1.3.7",
    "gulp-svgmin": "^1.2.4",
    "gulp.spritesmith": "^6.5.1",
    "merge-stream": "^1.0.1",
    "node-notifier": "^5.1.2",
    "node-sass-import-once": "^1.2.0",
    "npm-run-all": "^4.1.1",
    "path": "^0.12.7",
    "postcss-clean": "^1.0.3",
    "postcss-svg": "^2.1.0",
    "require-dir": "^0.3.2",
    "run-sequence": "^2.1.0",
    "vinyl-buffer": "^1.0.0"
  },
  "scripts": {
    "build": "npm-run-all build:svg build:css pattern-lab:css pattern-lab:twig build:notify",
    "build:css": "gulp build:css:development",
    "build:svg": "gulp svg:sprite",
    "build:notify": "gulp notifications:build",
    "build:pattern-lab": "npm-run-all pattern-lab:css pattern-lab:twig",
    "pattern-lab:css": "gulp pattern-lab:sass",
    "pattern-lab:twig": "php pattern-lab/core/console --generate",
    "production:build": "gulp build:css:production",
    "server:pattern-lab": "php pattern-lab/core/console --server",
    "a:sass": "gulp sass:components",
    "a:css": "gulp css:components",
    "a": "npm-run-all a:sass a:css build:pattern-lab"
  }
}
